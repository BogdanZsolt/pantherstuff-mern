{
  "name": "pantherstuff",
  "version": "1.0.0",
  "description": "eCommerce application built with the MEVN stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "server": "npm run server --prefix backend",
    "client": "npm run dev --prefix frontend",
    "build": "npm run build --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "preview": "concurrently \"npm run server\" \"npm run preview --prefix frontend\"",
    "data:import": "npm run data:import --prefix backend",
    "data:destroy": "npm run data:destroy --prefix backend"
  },
  "author": "Zsolt Bogd√°n",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^8.2.2",
    "mongoose": "^8.0.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
